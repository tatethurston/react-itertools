{
  "name": "react-itertools",
  "version": "0.0.3",
  "description": "A suite of tools for manipulating React children",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/react-itertools"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn webpack",
    "clean": "rm -rf dist",
    "lint": "yarn typecheck && prettier --check . && prettier-package-json --list-different '{,example/,}package.json' && eslint .",
    "lint:fix": "prettier --write . && prettier-package-json --write '{,example/}package.json' && eslint --fix .",
    "test": "yarn jest",
    "typecheck": "yarn tsc --noEmit",
    "version": "yarn run build && git add -A package.json",
    "postversion": "git push && git push --tags"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react": ">= 16"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.5",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.1",
    "prettier-package-json": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "keywords": [
    "react",
    "children",
    "recursive",
    "map",
    "map recursive",
    "all children",
    "iterate",
    "reactjs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
